<div class="form-wrap" id="small-page">
  <div class="in-form">
    <h1 class="name1">Edit</h1>
    <%= form_with model: @tag do |f| %>
    <%= f.text_field :name, class: "field" %>
    <%= f.submit "Update Tag", id: "btn" %>
    <% end %>
    <%= link_to "Go to Tag List", tags_path, id: "index-btn" %>
  </div>
</div>